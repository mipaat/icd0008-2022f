@page
@model DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>CheckersGame</h4>
    <hr/>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.WhitePlayerName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Entity.WhitePlayerName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.BlackPlayerName)
        </dt>
        <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Entity.BlackPlayerName)
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.WhiteAiType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Entity.WhiteAiType)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.BlackAiType)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Entity.BlackAiType)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.CheckersRulesetId)
        </dt>
        <dd class="col-sm-10">
            <a asp-page="/CheckersRulesets/Details" asp-route-id="@Model.Entity.CheckersRulesetId">@Html.DisplayFor(model => model.Entity.CheckersRulesetId)</a>
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.LastPlayed) (UTC)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Entity.LastPlayed)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.StartedAt) (UTC)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Entity.StartedAt)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.EndedAt) (UTC)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Entity.EndedAt)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.Winner)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Entity.Winner)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Entity.DrawProposedBy)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Entity.DrawProposedBy)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="/CheckersStates/Index" asp-route-checkersGameId="@Model.Entity.Id">View CheckersStates</a> |
    <a asp-page="./Edit" asp-route-id="@Model.Entity.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>